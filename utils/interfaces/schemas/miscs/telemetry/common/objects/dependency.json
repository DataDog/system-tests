{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "/miscs/telemetry/common/objects/dependency.json",
  "description": "Dependency Object",
  "title": "Instrumentation Telemetry Dependency",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "hash": {
      "type": "string"
    },
    "type": {
      "enum": ["SharedSystemLibrary", "PlatformStandard"],
      "example": "SharedSystemLibrary"
    },
    "version": {
      "type": "string"
    }
  },
  "required": ["name"]
}
