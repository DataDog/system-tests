{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "/miscs/telemetry/common/objects/logs.json",
  "description": "Logs Object",
  "title": "Instrumentation Telemetry Logs",
  "type": "object",
  "properties": {
    "message": {
      "type": "string"
    },
    "level": {
      "type": "string",
      "enum": ["ERROR", "WARN", "DEBUG"]
    },
    "tags": {
      "type": "string"
    },
    "stack_trace": {
      "type": "string"
    },
    "tracer_time": {
      "type": "number"
    }
  }
}
